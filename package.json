    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack-dev-server --config ./webpack/webpack_web.local.js -d --inline --hot --progress --no-info",
        "clean": "rimraf www/",
        "lint": "npm run lint-ts && npm run lint-js ",
        "lint-ts": "tslint --fix \"./src/**/*.tsx\" \"./src/**/*.ts\" ",
        "lint-js": "eslint \"./src/**/*.js\" --ignore-path .gitignore",
        "fixall": "tslint --fix \"./src/**\"",
        "RUN": "---------------------------------------RUN COMMANDS-----------------------------------",
        "run_web": "webpack-dev-server --config ./webpack/webpack_web.local.js -d --inline --hot --progress --no-info",
        "run_app": "webpack-dev-server --config ./webpack/webpack_app.local.js -d --inline --hot --progress --no-info",
        "run_device": "npm run build_app_dev && cordova run android --device && npm run remove_build",
        "run_device_copy": "npm run build_app_dev && npm run remove_build || cordova run android --device && npm run copy_build",
        "BUILD": "-------------------------------------BUILD COMMANDS-----------------------------------",
        "build_web_prod": "npm run clean &&  webpack -p --config ./webpack/webpack_web.prod.js",
        "build_web_stage": "npm run clean &&  webpack -p --config ./webpack/webpack_web.stage.js",
        "build_web_dev": "npm run clean &&  webpack --config ./webpack/webpack_web.dev.js",
        "build_app_prod": "npm run clean &&  webpack -p --config ./webpack/webpack_app.prod.js",
        "build_app_stage": "npm run clean &&  webpack -p --config ./webpack/webpack_app.stage.js",
        "build_app_dev": "npm run clean &&  webpack --config ./webpack/webpack_app.dev.js",
        "build_app": "npm run build_app_dev && npm run remove_build || cordova build android && npm run copy_build_local",
        "build_web": "npm run build_web_dev && zip -r www.zip www && mv www.zip /home/saisharath/dev/builds_ABP/builds/DCE/ABP_DCE_WEB_build_v\"$VERSION\"_$(date +\"%d-%m-%y_%I-%M\").zip",
        "build_both_local": "npm run build_app && zip -r ABP_DCE.zip ABP_DCE.apk www && npm run copy_both",
        "UTILITY": "-------------------------------------UTILITY COMMANDS-----------------------------------",
        "remove_build": "rm platforms/android/build/outputs/apk/debug/*.apk",
        "copy_build_local": "cp platforms/android/build/outputs/apk/debug/*.apk /home/saisharath/dev/builds_ABP/builds/DCE/ABP_DCE_APP_build_v\"$VERSION\"_$(date +\"%d-%m-%y_%I-%M\").apk",
        "copy_build_dev_git": "cp platforms/android/build/outputs/apk/debug/*.apk ABP_V$npm_package_config_APP_DEV_VERSION.apk",
        "copy_build_stage_git": "cp platforms/android/build/outputs/apk/debug/*.apk ABP_V$npm_package_config_APP_STAGE_VERSION.apk",
        "copy_build_prod_git": "cp platforms/android/build/outputs/apk/release/android-release.apk ABP_V$npm_package_config_APP_PROD_VERSION.apk",
        "copy_release-props": "cp ./release-signing.properties ./platforms/android/release-signing.properties",
        "copy_both": "cp ABP_DCE.zip /home/saisharath/dev/builds_ABP/builds/DCE/ABP_DCE.zip",
        "zip_both": "zip -r ABP_DCE.zip ABP_DCE.apk www",
        "set_clientID": "cordova plugin remove cordova-plugin-googleplus && cordova plugin add cordova-plugin-googleplus --variable REVERSED_CLIENT_ID=apps.googleusercontent.com.168387332597-9njemqld0nh5surje2ctg7ekgges2cdp",
        "GIT": "-------------------------------------GIT COMMANDS-----------------------------------",
        "build_app_dev_git": "npm run build_app_dev && cordova platforms add android --nofetch && npm run set_clientID && cordova build android && npm run copy_build_dev_git",
        "build_app_stage_git": "npm run build_app_stage && cordova platforms add android --nofetch && npm run set_clientID && cordova build android && npm run copy_build_stage_git",
        "build_app_prod_git": "npm run build_app_prod && cordova platforms add android --nofetch && npm run set_clientID && npm run copy_release-props && cordova build android --release && npm run copy_build_prod_git",
        "build_both_git": "npm run build_app_git && npm run build_web_dev",
        "copy_build": "cp platforms/android/build/outputs/apk/*.apk /home/saisharath/dev/builds_ABP/builds/SE/ABP_SE_build_v\"$VERSION\"_$(date +\"%d-%m-%y_%I-%M\").apk",
        "pretest": "npm run lint",
        "prebuild": "npm run clean",
        "postinstall": "typings install"
    },
